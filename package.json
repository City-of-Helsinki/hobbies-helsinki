    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "cross-fetch": "^3.1.5",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.4",
    "dotenv": "^16.0.1",
    "events-helsinki-components": "^0.0.5",
    "file-saver": "^2.0.5",
    "graphql": "^16.5.0",
    "hds-react": "^1.15.0",
    "ics": "^2.35.0",
    "jsdom": "^19.0.0",
    "msw": "^0.41.0",
    "next": "^12.2.0",
    "next-i18next": "^11.0.0",
