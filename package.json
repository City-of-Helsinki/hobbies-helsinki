{
  "name": "hobbies-helsinki",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "next lint --fix",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "lint": "next lint",
    "get-translations": "node ./scripts/get-translations.js",
    "generate:graphql": "DOTENV_CONFIG_PATH=./.env.local graphql-codegen -r dotenv/config --config codegen.yml",
    "// Refresh local changes from react-helsinki-headless-cms": "",
    "refresh:rhhc": "yarn --cwd ../react-helsinki-headless-cms build; rm -rf node_modules/react-helsinki-headless-cms/*; cp -rf ../react-helsinki-headless-cms/dist/* node_modules/react-helsinki-headless-cms",
    "// Refresh local changes from events-helsinki-monorepo core and compponents package": "",
    "refresh:monorepo": "yarn --cwd ../events-helsinki-monorepo/packages/core build; yarn --cwd ../events-helsinki-monorepo/packages/components build; yarn copy:monorepo",
    "copy:monorepo": "rm -rf node_modules/events-helsinki-core/*; cp -rf ../events-helsinki-monorepo/packages/core/dist/* node_modules/events-helsinki-core; rm -rf node_modules/events-helsinki-components/*; cp -rf ../events-helsinki-monorepo/packages/components/dist/* node_modules/events-helsinki-components",
    "test:browser": "testcafe \"chrome --window-size='1249,720'\" browser-tests/ --live --dev",
    "test:browser:wsl2win": "testcafe 'path:`/mnt/c/Program Files/Google/Chrome/Application/chrome.exe`' browser-tests/ --live --dev",
    "test:browser:ci": "testcafe \"chrome:headless --disable-gpu --window-size='1249,720'\" --screenshots path=report takeOnFails=true --video report browser-tests/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "@sentry/browser": "^7.1.1",
